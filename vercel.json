{
  "version": 2,
  "builds": [
    {
      "src": "apiopenai/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/songrecommender(.*)",
      "dest": "apiopenai/application.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.josedavidgt.com"
        }
      ],
      "destination": "https://josedavidgt.com/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.josedavidgt.com"
        }
      ],
      "destination": "https://josedavidgt.com/$1",
      "permanent": true
    },
    {
      "source": "https://josedavidgt.com/(.*)",
      "has": [
        {
          "type": "protocol",
          "value": "http"
        }
      ],
      "destination": "https://josedavidgt.com/$1",
      "permanent": true
    }
  ]
}