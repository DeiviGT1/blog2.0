{
  "version": 2,
  "builds": [
    {
      "src": "apiopenai/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "spotify/application.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/songrecommender(.*)", "destination": "/apiopenai/application.py" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "josedavidgt.com" }],
      "destination": "https://www.josedavidgt.com/:path*",
      "permanent": true
    }
  ]
}