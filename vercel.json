{
  "version": 3,
  "builds": [
    {
      "src": "apiopenai/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/songrecommender(.*)",
      "dest": "apiopenai/application.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "redirects": [
    {
      "source": "http://josedavidgt.com/:path*",
      "destination": "https://josedavidgt.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.josedavidgt.com/:path*",
      "destination": "https://josedavidgt.com/:path*",
      "permanent": true
    },
    {
      "source": "https://www.josedavidgt.com/:path*",
      "destination": "https://josedavidgt.com/:path*",
      "permanent": true
    }
  ]
}