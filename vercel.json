{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python",
        "config": { "pythonVersion": "3.9.6" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/app/static/$1",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.*)",
        "dest": "run.py"
      }
    ]
  }