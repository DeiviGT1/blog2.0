{
  "version": 2,
  "builds": [
    {
      "src": "apiopenai/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "sitemap.xml",
      "use": "@vercel/static"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/songrecommender(.*)", "destination": "/apiopenai/application.py" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "josedavidgt.com" }],
      "destination": "https://www.josedavidgt.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.josedavidgt.com" }],
      "destination": "https://www.josedavidgt.com/:path*",
      "permanent": true
    }
  ]
}