{
  "version": 2,
  "builds": [
    {
      "src": "apiopenai/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "sitemap.xml",
      "use": "@vercel/static"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/songrecommender(.*)", "dest": "apiopenai/application.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}