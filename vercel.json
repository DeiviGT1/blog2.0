{
  "version": 2,
  "builds": [
    {
      "src": "api/application.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/songrecommender(.*)", "dest": "api/application.py" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}
